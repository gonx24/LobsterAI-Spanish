{
  "name": "@lobsterai/skill-web-search",
  "version": "1.0.0",
  "description": "Web search skill using Playwright-controlled browser for real-time information retrieval",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server/index.js",
    "dev": "tsc && node dist/server/index.js",
     "postinstall": "npm run install:skills",
    "install:skills": "find SKILLs -name package.json -execdir npm install \\;",
    "build:skills": "find SKILLs -name tsconfig.json -execdir npx tsc -p {} \\;"
  },
  "keywords": [
    "lobsterai",
    "skill",
    "web-search",
    "playwright",
    "cdp"
  ],
  "author": "LobsterAI",
  "license": "MIT",
  "dependencies": {
    "express": "^4.22.1",
    "playwright-core": "^1.58.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/node": "^20.19.33",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.0"
  }
}
